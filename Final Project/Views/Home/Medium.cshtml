
@{
    ViewBag.Title = "Medium";
}

<h2>Medium</h2>

<form name="Medium" id="MedForm" action="About" method="get">
    First Name:
    <br>
    <input type="text" name="firstName">
    <br>
    <select name="medium" onchange="update_action(this)" >
        
        <option value="Painting">Painting</option>
        <option value="Photography">Photography</option>
        <option value="Decorative Art">Decorative Art</option>
        <option value="Sculpture">Sculpture</option>
    </select>
    <input type="submit" id="MedSubmit" value="Submit">
</form>